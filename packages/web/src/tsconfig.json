{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "jsx": "preserve",
    "allowJs": true,
    "isolatedModules": true,
    "declaration": false,
    "composite": false,
    "declarationMap": false,
    "noEmit": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "../server.js"
  ]
}
