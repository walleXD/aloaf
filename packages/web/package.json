{
  "name": "@loaf/web",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:app": "node devServer.js",
    "dev:pkg": "yarn build:pkg -w",
    "dev": "concurrently \"yarn dev:pkg\" \"yarn dev:app\"",
    "build:app": "next build src",
    "build:pkg": "tsc -b",
    "build": "concurrently \"yarn build:pkg\" \"yarn build:app\"",
    "start": "next start src",
    "export": "yarn build && next export src -o dist",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:w": "yarn generate --watch"
  },
  "dependencies": {
    "@loaf/web-auth": "*",
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "apollo-boost": "^0.4.3",
    "clsx": "^1.0.4",
    "formik": "^1.5.8",
    "formik-material-ui": "^0.0.20",
    "graphql": "^14.4.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.0.3",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^2.3.1",
    "next-with-apollo": "^4.2.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.4.0",
    "@graphql-codegen/fragment-matcher": "1.4.0",
    "@graphql-codegen/introspection": "1.4.0",
    "@graphql-codegen/typescript": "1.4.0",
    "@graphql-codegen/typescript-document-nodes": "1.4.0",
    "@graphql-codegen/typescript-operations": "1.4.0",
    "@graphql-codegen/typescript-react-apollo": "1.4.0",
    "@types/node": "^12.7.0",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "apollo": "^2.17.1",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "graphql-codegen-typescript-client": "^0.18.2",
    "http-proxy-middleware": "^0.19.1",
    "typescript": "^3.5.3"
  }
}
